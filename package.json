{
	"name": "soulsession",
	"description": "UI repo for soulsession. Powered by Vite + React.",
	"keywords": [
		"soulsession"
	],
	"repository": {
		"type": "git"
	},
	"version": "0.1.0",
	"scripts": {
		"setup": "git init && husky install",
		"format": "prettier !src/api* src/**/*.{ts,tsx} --write",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
		"dev": "vite",
		"storybook": "storybook dev -p 6006",
		"storybook:setup": "node node_modules/@storybook/addon-styling/bin/postinstall.js",
		"storybook:build": "storybook build",
		"test": "vitest run src/",
		"test:unit": "vitest src/",
		"test:unit:coverage": "vitest --coverage src/",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"commitlint": "commitlint --edit"
	},
	"dependencies": {
		"@hookform/resolvers": "3.3.0",
		"@radix-ui/react-checkbox": "^1.0.4",
		"@radix-ui/react-dialog": "^1.0.4",
		"@radix-ui/react-dropdown-menu": "^2.0.5",
		"@radix-ui/react-switch": "^1.0.3",
		"@radix-ui/themes": "1.1.2",
		"@stripe/react-stripe-js": "^2.3.0",
		"@stripe/stripe-js": "^2.1.5",
		"@tanstack/react-query": "4.33.0",
		"@tanstack/react-table": "8.9.3",
		"@videosdk.live/react-sdk": "^0.1.75",
		"axios": "^1.5.0",
		"i": "^0.3.7",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-error-boundary": "^4.0.11",
		"react-hook-form": "7.45.4",
		"react-hot-toast": "^2.4.1",
		"react-icons": "^4.10.1",
		"react-player": "^2.13.0",
		"react-router-dom": "6.15.0",
		"use-debounce": "^9.0.4",
		"vite-plugin-svgr": "^3.2.0",
		"zod": "3.22.2",
		"zustand": "4.4.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.7",
		"@commitlint/config-conventional": "^17.6.7",
		"@headlessui/react": "^1.7.15",
		"@hookform/devtools": "^4.3.1",
		"@storybook/addon-essentials": "^7.1.1",
		"@storybook/addon-interactions": "^7.1.1",
		"@storybook/addon-links": "^7.1.1",
		"@storybook/addon-styling": "^1.3.4",
		"@storybook/blocks": "^7.1.1",
		"@storybook/react": "^7.1.1",
		"@storybook/react-vite": "^7.1.1",
		"@storybook/testing-library": "0.0.14-next.2",
		"@tanstack/react-query-devtools": "4.33.0",
		"@tanstack/react-table-devtools": "^8.7.6",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@total-typescript/ts-reset": "^0.4.2",
		"@trivago/prettier-plugin-sort-imports": "^4.2.0",
		"@types/node": "^20.4.5",
		"@types/react": "^18.2.17",
		"@types/react-dom": "^18.2.7",
		"@types/testing-library__jest-dom": "^5.14.9",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"@vitest/coverage-v8": "^0.33.0",
		"autoprefixer": "^10.4.14",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-react": "^7.33.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"eslint-plugin-storybook": "^0.6.13",
		"eslint-plugin-unicorn": "^48.0.1",
		"husky": "^8.0.3",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"prettier-plugin-organize-imports": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"prop-types": "^15.8.1",
		"storybook": "^7.1.1",
